<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
	<mime-type type="application/x-vivado-project">
		<sub-class-of type="application/xml"/>
		<comment>Vivado Project</comment>
		<glob pattern="*.xpr"/>
		<icon name="com.xilinx.Vivado"/>
	</mime-type>
	<mime-type type="application/x-vivado-design-checkpoint">
		<sub-class-of type="application/zip"/>
		<comment>Vivado Design Checkpoint</comment>
		<glob pattern="*.dcp"/>
		<icon name="com.xilinx.Vivado"/>
	</mime-type>
	<mime-type type="application/x-vivado-waveform-configuration">
		<sub-class-of type="application/xml"/>
		<comment>Vivado Waveform Configuration</comment>
		<glob pattern="*.wcfg"/>
		<icon name="com.xilinx.Vivado"/>
	</mime-type>
	<mime-type type="application/x-vivado-waveform-database">
		<comment>Vivado Waveform Database</comment>
		<glob pattern="*.wdb"/>
		<icon name="com.xilinx.Vivado"/>
	</mime-type>
</mime-info>
